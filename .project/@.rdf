<stylesheet id="transform" version="1.0" xmlns="http://www.w3.org/1999/XSL/Transform">
	<template match="@*|node()" mode="clone">
		<copy>
			<apply-templates select="@*|node()" mode="clone"/>
		</copy>
	</template>
	<template match="node()" mode="text">
