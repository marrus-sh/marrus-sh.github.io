						<choose>
							<when test="@target and substring(@target, 1, 1)='#' and /.//tei:entry[@xml:id=substring(current()/@target, 2)]"><apply-templates select="/.//tei:entry[@xml:id=substring(current()/@target, 2)]/tei:form/tei:orth" /></when>
